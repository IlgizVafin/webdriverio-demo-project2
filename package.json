{
    "name": "webdriverio-demo-project",
    "version": "0.0.1",
    "description": "Demo project showing the bug in allure-report (cucumber step order)",
    "scripts": {
        "clean": "rm -rf errorShots/** allure-results/** allure-report/**",
        "test": "./node_modules/.bin/wdio",
        "lint": "./node_modules/.bin/eslint src/**/*.js",
        "generate-report": "./node_modules/.bin/allure generate ./allure-results --clean",
        "open-report": "./node_modules/.bin/allure open"
    },
    "keywords": [
        "webdriverio",
        "cucumber",
        "test",
        "selenium"
    ],
    "dependencies": {
        "babel-preset-es2015": "~6.24.0",
        "babel-register": "~6.26.0",
        "chai": "~4.1.0",
        "cucumber": "~2.2.0",
        "wdio-cucumber-framework": "~0.3.1",
        "wdio-selenium-standalone-service": "~0.0.9",
        "wdio-spec-reporter": "~0.1.0",
        "webdriverio": "4.7.1"
    },
    "devDependencies": {
        "allure-commandline": "^2.3.4",
        "babel-polyfill": "~6.26.0",
        "eslint": "~4.4.0",
        "eslint-config-airbnb-base": "~11.3.0",
        "eslint-plugin-import": "~2.7.0",
        "http-server": "~0.10.0",
        "minimist": "^1.2.0",
        "node-uuid": "^1.4.8",
        "wdio-allure-reporter": "^0.1.2"
    }
}
